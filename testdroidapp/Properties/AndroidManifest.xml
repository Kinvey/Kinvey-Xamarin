<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="1" android:versionName="1.0" package="com.kinvey.testdroidapp">
	<uses-sdk android:minSdkVersion="22"/>
	<uses-permission android:name="android.permission.GET_ACCOUNTS" />
	<uses-permission android:name="android.permission.AUTHENTICATE_ACCOUNTS" />
	<uses-permission android:name="android.permission.MANAGE_ACCOUNTS" />
	<uses-permission android:name="android.permission.USE_CREDENTIALS" />
	<uses-permission android:name="android.permission.ACCOUNT_MANAGER" />
	<application android:allowBackup="true" android:icon="@mipmap/icon" android:label="@string/app_name">
		<service android:name=".KinveyAccountService">
			<intent-filter>
				<action android:name="android.accounts.AccountAuthenticator" />
			</intent-filter>
			<meta-data android:name="android.accounts.AccountAuthenticator" android:resource="@drawable/KinveyAccountAuthenticator" />
		</service>
	</application>
</manifest>